<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="4dp"
        android:layout_marginStart="4dp"
        card_view:layout_constraintLeft_toLeftOf="parent"
        card_view:layout_constraintRight_toRightOf="parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textview_heading_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:paddingBottom="8dp"
                android:text="Eat Daily Analysis"
                card_view:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view_row_one_score_vs_session_item"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/cardview_border_for_table"
                card_view:layout_constraintBottom_toBottomOf="@+id/textview_date_row_one_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/textview_date_row_one_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_date_row_one_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="21/11/2017"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_heading_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_score_row_one_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="203"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_heading_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_date_row_two_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="21/11/2017"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_date_row_one_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_score_row_two_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="203"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_score_row_one_score_vs_session_item" />

            <View
                android:id="@+id/view_row_two_score_vs_session_item"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/cardview_border_for_table"
                card_view:layout_constraintBottom_toBottomOf="@+id/textview_date_row_two_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/textview_date_row_two_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_date_row_three_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="21/11/2017"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_date_row_two_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_score_row_three_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="203"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_score_row_two_score_vs_session_item" />

            <View
                android:id="@+id/view_row_three_score_vs_session_item"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/cardview_border_for_table"
                card_view:layout_constraintBottom_toBottomOf="@+id/textview_date_row_three_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/textview_date_row_three_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_date_row_four_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="21/11/2017"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_date_row_three_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_score_row_four_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="203"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_score_row_three_score_vs_session_item" />

            <View
                android:id="@+id/view_row_four_score_vs_session_item"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/cardview_border_for_table"
                card_view:layout_constraintBottom_toBottomOf="@+id/textview_date_row_four_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/textview_date_row_four_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_date_row_five_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="21/11/2017"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_date_row_four_score_vs_session_item" />

            <TextView
                android:id="@+id/textview_score_row_five_score_vs_session_item"
                style="@style/Base.TextAppearance.AppCompat.Body1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:paddingBottom="8dp"
                android:paddingTop="8dp"
                android:text="203"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toBottomOf="@+id/textview_score_row_four_score_vs_session_item" />

            <View
                android:id="@+id/view_row_five_score_vs_session_item"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/cardview_border_for_table"
                card_view:layout_constraintBottom_toBottomOf="@+id/textview_date_row_five_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/textview_date_row_five_score_vs_session_item" />

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:alpha="0.5"
                card_view:layout_constraintBottom_toBottomOf="@+id/view_row_five_score_vs_session_item"
                card_view:layout_constraintLeft_toLeftOf="parent"
                card_view:layout_constraintRight_toRightOf="parent"
                card_view:layout_constraintTop_toTopOf="@+id/view_row_five_score_vs_session_item"
                card_view:srcCompat="@drawable/ic_thin_arrowheads_pointing_down" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>

</android.support.constraint.ConstraintLayout>